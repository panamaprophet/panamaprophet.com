{
  "name": "panamaprophet.com",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "env:export": "set -a && source .env && set +a",
    "docker:build": "yarn env:export && docker build -t panamaprophet.com --build-arg CLIENT_ID --build-arg CLIENT_SECRET --build-arg REVALIDATION_INTERVAL .",
    "docker:run": "docker run -dp 3000:3000 panamaprophet.com"
  },
  "dependencies": {
    "next": "12",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sharp": "^0.30.4"
  },
  "devDependencies": {
    "@types/node": "^17.0.24",
    "@types/react": "^18.0.5",
    "typescript": "^4.2.3"
  }
}
